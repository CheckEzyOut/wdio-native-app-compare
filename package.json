{
  "name": "wdio-native-app-compare-service",
  "version": "1.0.1",
  "description": "Visual regression testing for Native iOS and Android apps with WebdriverIO",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint ./lib",
    "compile": "babel lib/ -d build/",
    "release": "np",
    "watch": "npm run compile -- --watch",
    "test.unit": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "webdriver.io",
    "native",
    "screenshots",
    "compare",
    "image",
    "android",
    "ios",
    "appium"
  ],
  "author": "wswebcreation.nl",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.6",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "inquirer": "^6.5.1",
    "jest": "^24.9.0",
    "np": "^5.0.3",
    "webdriverio": "^5.12.4"
  },
  "dependencies": {
    "@wdio/logger": "^5.12.1",
    "canvas": "^2.6.0",
    "chalk": "^2.4.2",
    "fs-extra": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wswebcreation/wdio-native-app-compare.git"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/lib/resemble/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**",
      "!lib/methods/addBlockOuts.js",
      "!lib/methods/checkBaselineImageExists.js",
      "!lib/methods/makeCroppedBase64Image.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 100,
        "lines": 97,
        "statements": 97
      }
    },
    "testEnvironment": "node"
  }
}
